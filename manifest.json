{
  "manifest_version": 2,
  "name": "Makesbridge Sales Assistant",
  "description": "Makesbridgeâ„¢",
  "version": "0.0.1.81",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons":
   {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
  "browser_action": {
    "default_title": "Makesbridge Sales Assistant"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "css": ["makesbridge_plugin.css","plugin_font_icon.css"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "jquery.min.js",
    "gmail.js",
    "jquery.highlight.js",
    "main.js",
    "img/*",
    "fonts/plugin_icon/*"
  ],
  "permissions": [
    "identity"
  ],
  "oauth2": {
      "client_id": "508252128097-rcvu2693lfpiobo33lrnr117bhvkhjs9.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/calendar",
        "https://www.googleapis.com/auth/calendar.readonly"
      ]
    },
	"key":""

}
